 <%= simple_form_for [@admin, @storefront], :html => { :class => "form-vertical offset1" }  do |f| %>

  <br>
  <br>
  <div class="field">
    <%= f.label :vendor_name %>
    <%= f.text_field :vendor_name, :class => 'span10' %>
  </div>
  <div class="field">
    <%= f.label :printing_capabilities %>
    <%= f.text_area :printing_capabilities, :class => 'span10', :rows => 6 %>
  </div>
  <div class="field">
    <%= f.label :cad_capabilities %>
    <%= f.text_area :cad_capabilities, :class => 'span10', :rows => 6 %>
  </div>
  <div class="field">
    <%= f.label :additional_details %>
    <%= f.text_area :description, :class => 'span10', :rows => 6 %>
  </div>
  <div class="actions">
    <%= f.submit :class => 'btn btn-primary sharp offset8' %>
  </div>
  <% end %>
  
